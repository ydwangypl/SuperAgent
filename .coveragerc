[run]
source = .
omit =
    tests/*
    test_*.py
    */tests/*
    */test_*.py
    */__pycache__/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    @abc.abstractmethod

[html]
directory = htmlcov

[json]
output = coverage.json
